{
  "ConnectionStrings": {
    "DefaultConnection": "YOUR CONNECTION STRING"
  },
  "DetailedErrors": true,
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "Microsoft.EntityFrameworkCore": "Information",
        "Microsoft.Hosting": "Information"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [ "Console", "Debug" ]
  }
}